public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		
		int arr[] = new int[5];
		
		arr[0] = 11;
		arr[1] = 12;
		arr[3] = 13;
		arr[4] = 14;
		
	    
	    int target = 12;
	    
	    for(int i=0; i<arr.length; i++){
	        
	        if(arr[i] == target){
	           
	            System.out.println(i);
	            break;
	        }
	    }
		
		
	
		
	}
}
