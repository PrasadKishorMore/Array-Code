
        int arr[] = {1,3,4,5};

        int n = arr.length + 1;

        int total = n*(n + 1)/2;

        System.err.println(total);

        int sum  = 0;

        for(int val : arr){

            sum += val;
        }

        int missval = total - sum;
        System.out.println("missval:"+missval);
