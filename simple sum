public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		
		int arr[] = new int[5];
		System.out.println(arr[0]);
		arr[0] = 12;
		arr[1] = 13;
		arr[2] = 14;
		arr[3] = 15;
		
		System.out.println(arr);
		
		int sum = 0;
		
		for(int i=0; i<arr.length; i++){
		    
		    System.out.println(arr[i]);
		    
		    sum +=arr[i];
		}
		
		System.out.println(sum);
		
	}
}
