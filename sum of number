  //sum of array
	    
	    Scanner ms = new Scanner(System.in);
	    
	    System.out.println("inter a five number:");
	    
	    int [] arr = new int[5];
	    
	    for(int i=0; i<arr.length; i++){
	        
	        arr[i]=ms.nextInt();
	    }
	    
		    
		  int sum =0;
		 for(int i=0; i<arr.length; i++){
		          sum+=arr[i];
		 }  
		 
		 System.out.println("sum of array index:"+sum);
