public class Main
{
	public static void main(String[] args) {
		System.out.println("Hello World");
		
		int arr[] = new int[5];
		
		arr[0] = 11;
		arr[1] = 12;
		arr[3] = 13;
		arr[4] = 14;
		
		int even = 0;
		int odd = 0;
		
		
		
		for(int i=0; i<arr.length; i++){
		    
		    if(arr[i]%2 == 0){
		        System.out.println("even");
		        even++;
		    }else{
		        System.out.println("odd");
		        odd++;
		    }
		}
		
		System.out.println(even +"+"+ odd);
		
	}
}
